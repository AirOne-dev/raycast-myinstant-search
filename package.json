{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "myinstant-soundboard",
  "title": "Soundboard",
  "description": "Play a sound from MyInstant",
  "icon": "myinstant-icon.png",
  "author": "Erwan-Martin",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search",
      "title": "Search",
      "description": "Search for a sound",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.9",
    "@raycast/utils": "^1.5.2",
    "cheerio": "^1.0.0-rc.12",
    "fs": "^0.0.1-security",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
